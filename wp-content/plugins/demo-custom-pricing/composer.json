{
	"name": "doubleedesign/demo-custom-pricing",
	"description": "A plugin to demo implementing and testing complex pricing rules in WooCommerce",
	"version": "0.0.1",
	"type": "wordpress-plugin",
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"require": {
		"php": ">=8.2"
	},
	"autoload": {
		"psr-4": {
			"Doubleedesign\\Pricing\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Doubleedesign\\Pricing\\Tests\\Unit\\": "__tests__/unit",
			"Doubleedesign\\Pricing\\Tests\\Integration\\": "__tests__/integration"
		}
	},
	"require-dev": {
		"brain/monkey": "^2.6",
		"pestphp/pest": "^3.8",
		"symfony/var-dumper": "^7.2",
		"10up/wp_mock": "^0.4.2",
		"guzzlehttp/guzzle": "^7.9",
		"vlucas/phpdotenv": "^5.6"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
